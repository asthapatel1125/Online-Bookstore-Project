<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bookstoreapplication.CustomerStartScreenController">
   <children>
      <Pane prefHeight="444.0" prefWidth="627.0">
         <children>
            <TableView fx:id="bookTable" layoutX="22.0" layoutY="53.0" prefHeight="298.0" prefWidth="584.0">
              <columns>
                <TableColumn fx:id="nameColumn" prefWidth="344.0" text="Book Title" />
                <TableColumn fx:id="priceColumn" prefWidth="136.0" text="Book Price" />
                  <TableColumn fx:id="checkBoxColumn" prefWidth="103.0" text="Select" />
              </columns>
            </TableView>
            <Button fx:id="buy" layoutX="37.0" layoutY="379.0" mnemonicParsing="false" onAction="#handleBuy" text="Buy" />
            <Button fx:id="RPnB" layoutX="109.0" layoutY="379.0" mnemonicParsing="false" onAction="#handleRPnBuy" text="Redeem Points and Buy" />
            <Button fx:id="logout" layoutX="297.0" layoutY="379.0" mnemonicParsing="false" onAction="#handleLogout" text="Logout" />
            <Label fx:id="label" layoutX="26.0" layoutY="13.0" prefHeight="29.0" prefWidth="542.0" text="Label" />
         </children>
      </Pane>
   </children>
</AnchorPane>
