<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bookstoreapplication.OwnerCustomerController">
   <children>
      <TableView fx:id="customerTable" layoutX="14.0" layoutY="14.0" prefHeight="475.0" prefWidth="1160.0">
        <columns>
          <TableColumn fx:id="username" prefWidth="400.0" sortable="false" text="Username" />
          <TableColumn fx:id="password" prefWidth="400.0" sortable="false" text="Password" />
            <TableColumn fx:id="points" prefWidth="285.0" text="Points" />
            <TableColumn fx:id="select" prefWidth="75.0" text="Select" />
        </columns>
      </TableView>
      <Label layoutX="48.0" layoutY="524.0" text="Username:">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label layoutX="48.0" layoutY="568.0" text="Password:">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="171.0" layoutY="527.0" prefWidth="250.0" />
      <TextField fx:id="passwordField" layoutX="171.0" layoutY="571.0" prefWidth="250.0" />
      <Button layoutX="48.0" layoutY="661.0" mnemonicParsing="false" onAction="#handleAdd" prefHeight="50.0" prefWidth="100.0" text="Add">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button layoutX="171.0" layoutY="661.0" mnemonicParsing="false" onAction="#handleDelete" prefHeight="50.0" prefWidth="100.0" text="Delete">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button layoutX="1033.0" layoutY="661.0" mnemonicParsing="false" onAction="#handleBack" prefHeight="50.0" prefWidth="100.0" text="Back">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
